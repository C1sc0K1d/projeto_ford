            <label class="chat__label" href="" class="text">Voltar</label>
          </div>
        </div>
      </form>
    </div>

    <div class="container" id="body">
      <div class="carro">
        <img id="ford" src="images/fordKa.svg" alt="">
      </div>
      <div class="infos">
        <span>Ford Ka</span>
        <img id="ford" src="images/InfoKa.png" alt="">
      </div>
    </div>

